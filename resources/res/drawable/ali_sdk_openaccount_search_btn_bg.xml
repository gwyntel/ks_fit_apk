<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_focused="true"
        android:state_pressed="true">
        <bitmap
            android:src="@drawable/openaccount_ic_search_pressed"
            android:tint="@color/ali_sdk_openaccount_hw_color_primary_dark"/>
    </item>
    <item android:state_enabled="false">
        <bitmap
            android:src="@drawable/openaccount_ic_search_disabled"
            android:tint="@color/ali_sdk_openaccount_hw_color_primary_disabled"/>
    </item>
    <item>
        <bitmap
            android:src="@drawable/openaccount_ic_search_normal"
            android:tint="@color/ali_sdk_openaccount_hw_color_primary"/>
    </item>
</selector>
